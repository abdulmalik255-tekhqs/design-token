{
  "name": "ryt-design-tokens",
  "version": "4.3.18",
  "description": "Your package description",
  "keywords": [
    "MetaMask",
    "Ethereum"
  ],
  "homepage": "https://github.com/abdulmalik255-tekhqs/design-token#readme",
  "bugs": {
    "url": "https://github.com/abdulmalik255-tekhqs/design-token/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdulmalik255-tekhqs/design-token.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:cjs": "tsc --project tsconfig.build.json --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --project tsconfig.build.json --module ESNext --outDir dist/esm",
    "build": "yarn build:cjs && yarn build:esm && cleancss -o dist/styles.css src/css/index.css",
    "release": "npm version patch && yarn build && npm publish --access public",
    "test": "jest && jest-it-up"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.14.5",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ryt-design-tokens": "4.3.14"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@1.22.22",
  "publishConfig": {
    "access": "public"
  }
}
